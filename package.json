{
	"name": "proxserve",
	"version": "2.1.0",
	"description": "Proxy Observe on objects and properties changes",
	"license": "Apache 2.0",
	"source": "src/index.ts",
	"main": "dist/commonjs/index.js",
	"module": "dist/esmodule/index.js",
	"moduleMinified": "dist/esmodule/index.min.js",
	"types": "dist/commonjs/index.d.ts",
	"scripts": {
		"start": "node index.js",
		"test": "jest",
		"typecheck": "tsc src/*.ts --noEmit",
		"build": "parcel build && cp -f -t ./dist/esmodule ./dist/commonjs/index.d.ts ./dist/commonjs/index.d.ts.map",
		"watch": "parcel watch"
	},
	"targets": {
		"main": {
			"outputFormat": "commonjs",
			"engines": {
				"node": ">=13.0.0"
			},
			"context": "node",
			"optimize": false,
			"isLibrary": true
		},
		"module": {
			"outputFormat": "esmodule",
			"engines": {
				"browsers": "> 0.5%, last 2 versions, not dead"
			},
			"context": "browser",
			"optimize": false,
			"isLibrary": true
		},
		"moduleMinified": {
			"outputFormat": "esmodule",
			"engines": {
				"browsers": "> 0.5%, last 2 versions, not dead"
			},
			"context": "browser",
			"optimize": true,
			"isLibrary": true
		}
	},
	"devDependencies": {
		"@parcel/packager-ts": "^2.7.0",
		"@parcel/transformer-typescript-types": "^2.7.0",
		"@tsconfig/recommended": "^1.0.1",
		"@types/jest": "^27.5.2",
		"jest": "^27.5.1",
		"lodash": "^4.17.21",
		"parcel": "^2.7.0",
		"ts-jest": "^27.1.5",
		"typescript": "^4.8.4"
	},
	"engines": {
		"node": ">=13.0.0"
	},
	"keywords": [
		"proxy",
		"observe",
		"proxy observer",
		"two way binding"
	],
	"author": "Noam Lin",
	"contributors": [
		"Noam Lin <noamlin@gmail.com>"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/noamlin/proxserve"
	},
	"babel": {},
	"browserslist": [
		"last 1 Chrome version"
	]
}
